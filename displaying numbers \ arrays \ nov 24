#include <iostream>
using namespace std;
int main() {
    int num [5];
    
    cout<< "Enter Numbers:__  \n " ;
    for ( int i=0; i<5; ++i) {
         cin >> num [i] ;
    }
    
    cout << " Your numbers are as following:___ \n  " ;
    
    for (int n=0; n<5; ++n) {
        
        cout << num[n] << " " ;
    }
  
}
